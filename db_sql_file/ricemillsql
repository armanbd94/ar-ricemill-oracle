  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTIONS" MODIFY ("DELETABLE" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTIONS" ADD CHECK (type in ('1', '2', '3')) ENABLE;
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTIONS" ADD CHECK (status in ('1', '2')) ENABLE;
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTIONS" ADD CHECK (deletable in ('1', '2')) ENABLE;
