--  DDL for Trigger ALLOWANCE_DEDUCTION_MANAGES_ID
--------------------------------------------------------

  CREATE OR REPLACE NONEDITIONABLE TRIGGER "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES_ID" 
            before insert on ALLOWANCE_DEDUCTION_MANAGES
            for each row
                begin
            if :new.ID is null then
                select allowance_deduction_manages_id.nextval into :new.ID from dual;
            end if;
            end;

/
