--  Constraints for Table ALLOWANCE_DEDUCTION_MANAGES
--------------------------------------------------------

  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("ID" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("ALLOWANCE_DEDUCTION_ID" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("EMPLOYEE_ID" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("BASIC_SALARY" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("PERCENTAGE" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("AMOUNT" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("STATUS" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" MODIFY ("DELETABLE" NOT NULL ENABLE);
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" ADD CHECK (status in ('1', '2', '3')) ENABLE;
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" ADD CHECK (deletable in ('1', '2')) ENABLE;
  ALTER TABLE "SYSTEM"."ALLOWANCE_DEDUCTION_MANAGES" ADD CONSTRAINT "ALLOWANC_DEDUCTIO_MANAGE_ID_PK" PRIMARY KEY ("ID")
  USING INDEX "SYSTEM"."ALLOWANC_DEDUCTIO_MANAGE_ID_PK"  ENABLE;
